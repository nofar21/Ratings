@model Rating
<ul>
    <li>Id: @Model.Id</li>
    <li>Name Of Rater: @Model.NameOfRater</li>
    <li>Rate Number: @Model.RateNumber</li>
    <li>Feedback: @Model.Feedback</li>
    <li>Time: @Model.Time</li>
</ul>
<form asp-action="Delete">
    <label>Are you sure you want to delete this rating?</label>
    <input class="btn btn-outline-light" type="submit" value="Delete" />
</form>
<form asp-action="Index">
    <label>If the answer is no...</label>
    <input class="btn btn-outline-light" type="submit" value="Return"/>
</form>